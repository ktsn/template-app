{
  "name": "template-app",
  "version": "1.0.0",
  "author": "katashin",
  "private": true,
  "scripts": {
    "prepublishOnly": "npm run test && npm run clean && npm run build",
    "clean": "rm -rf dist",
    "build": "webpack --progress --config scripts/webpack.config.prod.js",
    "dev": "webpack-dev-server --stdin --hot --colors --config scripts/webpack.config.dev.js",
    "dev:test": "karma start scripts/karma.conf.js",
    "lint": "eslint --ext js,vue src test scripts",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "karma start scripts/karma.conf.js --single-run"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.0.0",
    "eslint-config-ktsn": "^1.0.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "glob": "^7.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "power-assert": "^1.4.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "uglify-save-license": "^0.4.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-espower-loader": "^1.0.2",
    "webpack-merge": "^4.1.0"
  }
}
